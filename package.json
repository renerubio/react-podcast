{
  "name": "podcasts",
  "version": "0.1.0",
  "private": true,
  "author": "René Rodolfo Rubio Muñoz",
  "email": "renerubio.dev@hotmail.com",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "prod": "cross-env NODE_ENV=production next build && cross-env NODE_ENV=production next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepush": "npm run -s lint -- --max-warnings=0 && npm run -s typecheck && npm run -s test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css,scss}": "prettier --write"
  },
  "dependencies": {
    "@tanstack/query-async-storage-persister": "5.90.2",
    "@tanstack/query-persist-client-core": "5.90.2",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-query-persist-client": "5.90.2",
    "idb-keyval": "6.2.2",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "5.90.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "20",
    "@types/react": "19.2.2",
    "@types/react-dom": "19",
    "@vitest/coverage-v8": "4.0.3",
    "babel-plugin-react-compiler": "1.0.0",
    "cross-env": "10.1.0",
    "eslint": "9",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "jsdom": "27.0.1",
    "lint-staged": "16.2.6",
    "next": "^16.0.10",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "typescript": "5.9.3",
    "vitest": "4.0.3"
  }
}
